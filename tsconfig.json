{
    "compilerOptions": {
        "strict": true,
        "declaration": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "jsx": "react",
        "baseUrl": "./",
        "paths": {
            "@@/*": [
                ".dumi/tmp/*"
            ],
            "libsx": [
                "src"
            ],
            "libsx/*": [
                "src/*",
                "*"
            ]
        }
    },
    "include": [
        ".dumirc.ts",
        "src/**/*"
    ]
}